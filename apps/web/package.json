    "chokidar": "^3.5.1",
    "dotenv-cli": "^4.0.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^2.0.0",
    "raw-loader": "^4.0.2",
    "sitemap": "^7.0.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  }
