    "build": "next build && next export -o .dist && yarn copy-images && yarn sitemap",
    "start": "next start",
    "sitemap": "dotenv -- ts-node --project tsconfig.server.json src/utils/createSitemap.ts",
    "copy-images": "dotenv -- ./scripts/copy-images.sh"
  },
  "dependencies": {
