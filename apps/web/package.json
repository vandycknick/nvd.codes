    "@grpc/grpc-js": "^1.3.2",
    "@nvd.codes/core": "^1.0.0",
    "@nvd.codes/blog-proto": "1.0.0",
    "framer-motion": "^4.1.17",
    "next": "^10.2.2",
    "next-seo": "^4.24.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-markdown": "^5.0.3",
    "react-syntax-highlighter": "^15.4.3",
    "remark-gfm": "^1.0.0",
    "swr": "^0.5.6",
    "timeago.js": "^4.0.2"
